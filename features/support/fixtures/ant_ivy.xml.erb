<ivy-module version="2.0">
    <info organisation="net.twasink.<%=group%>" module="<%=name.downcase%>" />

    <dependencies>
<% dependencies.each() { | dep |%>
        <dependency org="net.twasink.<%=group%>" name="<%=dep[:name].downcase%>" rev="<%=dep[:version]%>"/>
<%}%>
<% forced_dependencies.each() { | dep |%>
        <dependency org="net.twasink.<%=group%>" name="<%=dep[:name].downcase%>" rev="<%=dep[:version]%>" force="true"/>
<%}%>
    </dependencies>

</ivy-module>